{
  "port": 8090,
  "server": [
    {
      "host": "docker-graph.traefik.me",
      "port": 8080,
      "target": "./main.go",
      "resources": {
        "paths": [
          "main.go",
          "web",
          "docker"
        ],
        "ignore": "static*"
      },
      "assets": {
        "paths": [
          "web/static"
        ]
      },
      "startup": [
        "-dev"
      ]
    }
  ]
}
